{{page-title "Search"}}

<form>
  <SearchInput
    @label="Search"
    @value={{this.q}}
    @onChange={{this.handleSearch}}
  />
  <LinkTo
    @route="search.filter"
    @models={{array this.type}}
    @query={{this.defaults}}
  >
    reset
  </LinkTo>
</form>
{{log this.type}}
{{log this.defaults}}
<nav aria-label="search results">
  <LinkTo @route="search.filter" @models={{array "foo"}}>Foo</LinkTo>
  <LinkTo @route="search.filter" @models={{array "bar"}}>Bar</LinkTo>
</nav>

{{outlet}}